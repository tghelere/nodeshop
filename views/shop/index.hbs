{{# each products}}
    <div class="row">
        {{# each this}}
            <div class="col-md-3">
                <div class="card">
                    <img class="card-img-top thumb-product img-responsive" src="{{ this.imagePath }}" alt="Card image cap">
                    <div class="card-body">
                        <h4 class="card-title">{{ this.title }}</h4>
                        <p class="card-text description-product">{{ this.description }}</p>
                        <div>
                            <div class="price-product pull-left">R${{ this.price }}</div>
                            <a href="#" class="btn btn-success pull-right">
                                <i class="fa fa-plus-circle" aria-hidden="true"></i> Pedido</a>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}        
    </div>
{{/each}}

